<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/com_tnk_off_adlist_empty_alert"
        android:layout_width="180dp"
        android:layout_height="120dp"
        android:layout_marginTop="100dp"
        android:src="@drawable/com_tnk_offerwall_alert"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/com_tnk_adlist_no_item_script"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="100dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="참여할 수 있는 미션을\n준비 중이에요!"
        style="@style/com_tnk_offerwall_message_1"
        app:layout_constraintLeft_toLeftOf="@id/com_tnk_off_adlist_empty_alert"
        app:layout_constraintRight_toRightOf="@id/com_tnk_off_adlist_empty_alert"
        app:layout_constraintTop_toBottomOf="@id/com_tnk_off_adlist_empty_alert" />

</androidx.constraintlayout.widget.ConstraintLayout>
